            <Label fx:id="clockLabel" layoutX="498.0" layoutY="14.0" prefHeight="48.0" prefWidth="238.0" text="Label" textAlignment="RIGHT">
               <font>
                  <Font size="30.0" />
               </font>
               <effect>
                  <Glow level="0.21" />
               </effect>
            </Label>
         </children>
      </Pane>
      <Pane prefHeight="600.0" prefWidth="200.0">
         <children>
            <Button layoutX="48.0" layoutY="40.0" mnemonicParsing="false" onAction="#imageSelect" prefHeight="36.0" prefWidth="105.0" text="Select Image" />
            <CheckBox fx:id="checkBox" layoutX="90.0" layoutY="13.0" mnemonicParsing="false" onAction="#onChecked" selected="true" text="Clock" />
            <Slider fx:id="Rbar" layoutX="30.0" layoutY="115.0" max="255.0" onMouseReleased="#baseBarsChanged" value="128.0">
