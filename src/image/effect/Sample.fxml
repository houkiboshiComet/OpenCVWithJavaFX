<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<SplitPane dividerPositions="0.5" prefHeight="643.0" prefWidth="958.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="image.effect.SampleController">
	<items>
		<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="642.0" prefWidth="750.0" style="-fx-background-color: white;">
			<children>
				<ImageView fx:id="imageView" fitHeight="600.0" fitWidth="720.0" layoutX="12.0" layoutY="31.0" nodeOrientation="INHERIT" onMouseClicked="#onImageClicked" pickOnBounds="true" preserveRatio="true" />
				<Label fx:id="clockLabel" layoutX="462.0" layoutY="31.0" prefHeight="48.0" prefWidth="238.0" text="Label" textAlignment="RIGHT">
					<font>
						<Font size="30.0" />
					</font>
				</Label>
				<CheckBox fx:id="checkBox" layoutX="682.0" layoutY="6.0" mnemonicParsing="false" onAction="#onChecked" selected="true" text="Clock" />
				<ColorPicker fx:id="colorPicker" editable="true" layoutX="569.0" layoutY="3.0" onAction="#onClockColorChanged" prefHeight="23.0" prefWidth="97.0">
				</ColorPicker>
			</children>
		</Pane>
		<Pane prefHeight="625.0" prefWidth="200.0">
			<children>
				<Button layoutX="48.0" layoutY="5.0" mnemonicParsing="false" onAction="#imageSelect" prefHeight="36.0" prefWidth="105.0" text="Select Image" />
				<Slider fx:id="Rbar" layoutX="30.0" layoutY="151.0" max="255.0" onMouseReleased="#baseBarsChanged" value="128.0">
					<effect>
						<ColorAdjust brightness="0.56" contrast="0.03" hue="0.02" saturation="1.0" />
					</effect>
				</Slider>
				<Button layoutX="25.0" layoutY="568.0" mnemonicParsing="false" onAction="#test" text="Button" />
				<Slider fx:id="Gbar" layoutX="30.0" layoutY="181.0" max="255.0" onMouseReleased="#baseBarsChanged" value="128.0">
					<effect>
						<ColorAdjust brightness="0.35" hue="0.65" saturation="0.49" />
					</effect>
				</Slider>
				<Slider fx:id="Bbar" layoutX="30.0" layoutY="213.0" max="255.0" onMouseReleased="#baseBarsChanged" value="128.0">
					<effect>
						<ColorAdjust brightness="0.51" hue="-0.51" saturation="1.0" />
					</effect>
				</Slider>
				<Label layoutX="44.0" layoutY="122.0" text="-  RGB brightness  +" />
				<Label layoutX="34.0" layoutY="244.0" text="Sharpness             Blur" />
				<Slider fx:id="blurBar" blockIncrement="1.0" layoutX="34.0" layoutY="263.0" max="10.0" min="-10.0" onMouseReleased="#baseBarsChanged" />
				<Button layoutX="52.0" layoutY="503.0" mnemonicParsing="false" onAction="#saveImage" prefHeight="36.0" prefWidth="105.0" text="Save Image" />
				<Separator layoutX="1.0" layoutY="290.0" prefWidth="200.0" />
            <ComboBox fx:id="comboBox" layoutX="30.0" layoutY="325.0" prefWidth="150.0" promptText="None" />
            <Label layoutX="58.0" layoutY="303.0" text="Practical Effects" />
            <Slider fx:id="effectBar" layoutX="30.0" layoutY="358.0" onMouseReleased="#effectBarChanged" />
            <Button layoutX="47.0" layoutY="56.0" mnemonicParsing="false" onAction="#onReset" prefHeight="36.0" prefWidth="105.0" text="Reset Effects" />
            <Separator layoutX="1.0" layoutY="396.0" prefWidth="200.0" />
            <ComboBox fx:id="stampTypes" layoutX="25.0" layoutY="439.0" prefWidth="150.0" promptText="None" />
            <Label layoutX="69.0" layoutY="413.0" text="Stamp Type" />
			</children>
		</Pane>
	</items>
</SplitPane>
