            </Slider>
            <Slider fx:id="Bbar" layoutX="30.0" layoutY="177.0" max="255.0" onMouseReleased="#baseBarsChanged" value="128.0">
               <effect>
                  <ColorAdjust brightness="0.51" hue="-0.51" saturation="1.0" />
               </effect>
            </Slider>
